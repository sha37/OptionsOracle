<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<indicator xmlns="http://www.samoasky.com/optionsoracle_wizard.xml" version="1.4.0.2">
  <indicator name="A/B Spread">
    <IndicatorTable>
    <Enable>1</Enable>
    <Name>A/B Spread</Name>
    <Equation>(Option.Ask - Option.Bid)*2 / (Option.Ask + Option.Bid)</Equation>
    <Format>P2</Format>
    <Description>http://www.samoasky.com/optionsoracle_wizard/strategies.html</Description>
     </IndicatorTable>
  </indicator>
   <indicator name="Days to Expiry">
    <IndicatorTable>
    <Enable>1</Enable>
    <Name>Days to Expiry</Name>
    <Equation>Math.ceil(Option.TimeToExpiration*365)</Equation>
    <Format>N0</Format>
    <Description>http://www.samoasky.com/optionsoracle_wizard/strategies.html</Description>
     </IndicatorTable>
  </indicator>
  <indicator name="Elasticity">
    <IndicatorTable>
    <Enable>1</Enable>
    <Name>Elasticity</Name>
    <Equation>(Option.Delta*Stock.Last*Option.Bid) / (Option.Ask*Option.Ask)</Equation>
    <Format>N2</Format>
    <Description>http://www.samoasky.com/optionsoracle_wizard/strategies.html</Description>
     </IndicatorTable>
  </indicator>
</indicator>
